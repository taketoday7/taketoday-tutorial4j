<?xml version="1.0" encoding="UTF-8"?>
<configuration>
   <include
         resource="org/springframework/boot/logging/logback/base.xml"/>
   <appender name="LISTAPPENDER"
             class="cn.tuyucheng.taketoday.webclient.utils.ListAppender">
   </appender>
   <logger
         name="cn.tuyucheng.taketoday.webclient.clientcredentials.service.WebClientChonJob">
      <appender-ref ref="LISTAPPENDER"/>
   </logger>
   <root level="info">
      <appender-ref ref="CONSOLE"/>
      <appender-ref ref="LISTAPPENDER"/>
   </root>
</configuration>